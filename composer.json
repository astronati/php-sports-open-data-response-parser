{
  "name": "astronati/sports-open-data-response-parser",
  "description": "Allows to map responses provided by the Sports Open Data API.",
  "type": "library",
  "license": "MIT",
  "version": "1.2.0",
  "authors": [
    {
      "name": "Andrea Stronati",
      "email": "astronati@vendini.com"
    }
  ],
  "minimum-stability": "stable",
  "require-dev": {
    "codacy/coverage": "^1.4",
    "consolidation/robo": "~1",
    "phpunit/phpunit": "^6.5",
    "phpunit/php-code-coverage": "^5.3",
    "phpstan/phpstan": "^0.9.1"
  },
  "require": {
    "php": ">=7.1.0"
  },
  "autoload": {
    "psr-4": {
      "SODRP\\": "src"
    },
    "classmap": [
      "src/"
    ],
    "exclude-from-classmap": [
      "/tests/**/*.php"
    ]
  },
  "scripts": {
    "analysis": "./vendor/bin/robo analysis",
    "coverage": "./vendor/bin/robo coverage:open",
    "test": "./vendor/bin/robo test"
  }
}
